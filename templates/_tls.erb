

# Transport Layer Security (TLS)
#

TLSProtocolMin <%= @tls_protocol_min %>
TLSCipherSuite <%= @tls_cipher_suite %>

TLSCACertificateFile <%= @tls_ca_certificate_file %>
TLSCertificateFile <%= @tls_certificate_file %>
TLSCertificateKeyFile <%= @tls_key_file %>

<% if @tls_dh_param_file -%>
TLSDHParamFile <%= @tls_dh_param_file %>
<% end -%>

